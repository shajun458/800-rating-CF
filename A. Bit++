#include<bits/stdc++.h>
using namespace std ;
int main ()
{
   int n,k,i,j,c=0;
   cin >>n;

   string  a[n];

   for (i=0; i<n; i++)
   {
       cin>>a[i];

   }

   for (j=0; j<n; j++){
    if (a[j]=="++X" || a[j]=="X++" )
    c++;
   else if (a[j]== "--X" || a[j]== "X--")
    c--;


   }

   cout << c;


}
