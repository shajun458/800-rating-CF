#include<bits/stdc++.h>
using namespace std;
int main ()
{
    int n,c=0;
    cin>>n;
    int a[4];
    for (int i=0 ;i<n; i++)
    {
        int sum=0;
        for(int j=0;j<3;j++){
        cin>>a[j];
        sum=sum+a[j];
        }
        if (sum>1)
        {
            c++;
        }
    }
 cout<<c<<endl;
}
 
